{
  "expo": {
    "name": "SolcodeApp",
    "slug": "SolcodeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/AppLogo.png",
    "splash": {
      "image": "./assets/defaults/Splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "br.com.solcode.outsourcing"
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    },
    "android": {
      "package": "br.com.solcode.outsourcing"
    }
  }
}
